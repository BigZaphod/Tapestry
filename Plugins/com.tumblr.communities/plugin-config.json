{
	"id": "com.tumblr.communities",
	"display_name": "Tumblr - Follow Community",
	"service_name": "Tumblr",
	"default_color": "green",
	"icon": "https://usetapestry.com/icons/tumblr.png",
	"item_style": "post",
	"site": "https://api.tumblr.com",
	"oauth_authorize": "https://www.tumblr.com/oauth2/authorize",
	"oauth_token": "https://api.tumblr.com/v2/oauth2/token",
	"oauth_type": "code",
	"oauth_code_key": "code",
	"oauth_scope": "write+offline_access",
	"oauth_grant_type": "authorization_code",
	"oauth_http_redirect": true,
    "synchronizable_credentials": false,
	"fail_status_code": 401,
	"needs_verification": true,
	"verify_variables": true,
	"provides_attachments": true,
	"check_interval": 300,
	"minimum_app_version": "1.4",
	"version": 2
}
